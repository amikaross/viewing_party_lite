<% if current_user %>
  <%= link_to "Log Out", "/logout" %>

  <div id='users'>
    <h2>Existing Users:</h2>
    <ul>
      <% @users.each do |user| %>
        <li><b>Name:</b> <%= user.name %><br><b>Email:</b> <%= user.email %> </li><br>
      <% end %>
    </ul>
  </div>
<% else %>
  <%= button_to "Create A New User", register_path, method: :get %>
  <%= link_to "Log In", "/login" %>
<% end %>